/include/ "dsi_panel_innolux_1080p_video.dtsi"
/include/ "msm8974-acer-leds.dtsi"
/include/ "msm8974-acer-camera-sensor.dtsi"

&soc {
	serial@f991e000 {
		status = "ok";
	};

	qcom,mdss_dsi_innolux_1080p_video {
		status = "ok";
		qcom,cont-splash-enabled;
	};

	qcom,hdmi_tx@fd922100 {
		status = "ok";
	};

	spi@f9967000 {
		ntrig_spi@0{
			compatible = "ntrig,ntrig-spi";
			reg = <0>;
			spi-max-frequency = <10000000>;
			interrupt-parent = <&msmgpio>;
			interrupts = <61 0x2>;
			vcc_spi-supply = <&pm8941_lvs1>;
		};
	};

	i2c@f9923000 {
		isl98607@29 {
			compatible = "qcom,isl98607";
			reg = <0x29>;
			smps3a-supply = <&pm8941_s3>;
		};
	};

	i2c@f9924000 {
/*
		cypress-ts@24 {
			compatible = "cypress,cypress-ts";
			reg = <0x24>;
			interrupt-parent = <&msmgpio>;
			interrupts = <77 0x2>;
			vdd_ana-supply = <&pm8941_l18>;
			vcc_i2c-supply = <&pm8941_lvs1>;
			cypress,reset-gpio = <&msmgpio 60 0x00>;
			cypress,irq-gpio = <&msmgpio 77 0x00>;
			cypress,panel-coords = <0  0 1080 2016>;
			cypress,display-coords = <0 0 720 1280>;
			cypress,i2c-pull-up;
		};
*/

		cypress_capsense@2c {
			compatible = "cypress,capsense-key";
			reg = <0x2c>;
			interrupt-parent = <&msmgpio>;
			interrupts = <73 0x2>;
			vcc_i2c-supply = <&pm8941_lvs1>;
			cypress,reset-gpio = <&msmgpio 13 0x00>;
			cypress,irq-gpio = <&msmgpio 73 0x02>;
		};
	};

	gen-vkeys {
		compatible = "qcom,gen-vkeys";
		label = "atmel_mxt_ts";
		qcom,disp-maxx = <720>;
		qcom,disp-maxy = <1280>;
		qcom,panel-maxx = <760>;
		qcom,panel-maxy = <1424>;
		qcom,key-codes = <158 139 102 217>;
	};

	i2c@f9928000 {
		pn544@28 {
			compatible = "nxp,pn544";
			reg = <0x28>;
			interrupt-parent = <&msmgpio>;
			interrupts = <59 0x1>;
			nfc_vdd-supply = <&pm8941_s3>;
			nfc_vsim-supply  = <&pm8941_l9>;
			irq-gpio = <&msmgpio 59 0x00>;
		};
	};
